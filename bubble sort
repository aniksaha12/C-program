#include<stdio.h>
void main()
{
int a[100];
int n,i,j,t;
system("clear");
printf("\n Enter number of elements:");
scanf("%d",&n);
        for(i=0;i<n;i++)
        scanf("%d",&a[i]);

printf("\n Before sorting:");
            for(i=0;i<n;i++)
            printf("%d",a[i]);
                for(i=0;i<n;i++)

    {
        for(j=0;j<n-i-1;j++)
        {
            if(a[j]>a[j+1])
            {
                t=a[j];
                a[j]=a[j+1];
                a[j+1]=t;
         }
    }

}
        printf("\n After sorting:");
            for(i=0;i<n;i++)
              printf("%d",a[i]);
}
