#include<stdio.h>
void main()
{

    int a,i,flag=1;
    printf("Enter the number ::");
    scanf("%d",&a);
    for(i=2;i<a/2;i++)
    {
        if(a%i==0)
        {
            flag=0;
            break;

        }
    }
   if(flag==0)
   {
       printf("The number is not prime");

   }
     else
     {
         printf("The number is prime");
     }
     }
