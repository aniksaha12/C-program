void check palindrom(char*A)
{
    int l,i,j;
    i=0
    while([i]!='\0')
        i++;
    l=i;
    j=l-1;
    for(i=0;i,=l/2;i++)
    {
        if(A[i]!=A[j--])
            printf("Not palindrom":);
        f=1;
        break;
    }
}
{
    if(f==0)
        printf("String are palindrom":);
